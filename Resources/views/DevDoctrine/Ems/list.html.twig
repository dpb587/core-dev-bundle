<table class="table">
    <thead>
        <tr>
            <th>Name</th>
        </tr>
    </thead>
    <tbody>
        {% for result in results %}
            <tr>
                <td>
                    <a href="{{ path('dpb_coredev_devtwig_filter_index', { 'filter' : result.name }) }}"{% if result.default %} style="font-weight:bold;"{% endif %}><code>{{ result.name }}</code></a>
                </td>
            </tr>
        {% endfor %}
    </tbody>
</table>
